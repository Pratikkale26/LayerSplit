    "@types/helmet": "^0.0.48",
    "@types/node": "^25.2.1",
    "@types/pg": "^8.16.0",
    "cors": "^2.8.6",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "helmet": "^8.1.0",
    "pg": "^8.18.0",
